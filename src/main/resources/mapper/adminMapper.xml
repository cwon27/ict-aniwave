<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ict.finalproject.DAO.TAdminDAO">
    <!-- 관리자 ID 존재 여부 확인 -->
    <select id="existsByAdminId" resultType="boolean">
        SELECT COUNT(*) > 0
        FROM t_admin
        WHERE adminid = #{adminid}
    </select>
</mapper>